// To find the array type

#include<stdio.h>
int main()
{
    int n,a[100],i,count=0,count1=0;
    scanf("%d",&n);
    for(i=1;i<=n;i++)
    {
        scanf("%d",&a[i]);
    }
    for(i=1;i<=n;i++)
    {
        if(a[i]%2==0)
        {
           count++;
        }
        if(a[i]%2!=0)
        {
            count1++;
        }
    }
    if(count==n)
    {
        printf("even");
    }
    else if(count1==n)
    {
        printf("odd");
    }
    else
    {
        printf("mixed");
    }
}
