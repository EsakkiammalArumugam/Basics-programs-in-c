// Find number of even and odd elements in an array

#include<stdio.h>
int main()
{
    int n,a[100],i,count=0,count1=0;
    scanf("%d",&n);
    for(i=1;i<=n;i++)
    {
        scanf("%d",&a[i]);
    }
    for(i=1;i<=n;i++)
    {
        if(a[i]%2==0)
        {
            count++;
        }
        else
        {
            count1++;
        }
    }
    printf("Odd:%d Even:%d",count1,count);
}
