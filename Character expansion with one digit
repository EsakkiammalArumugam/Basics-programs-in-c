/*** Character expansion
b6j4k3
bbbbbbjjjjkkk
********/

#include<stdio.h>
int main()
{
    char s[100],i,temp,j,temp1;
    scanf("%[^\n]s",s);
    for(i=0;s[i]!='\0';i++)
    {
        if(s[i]>='0' && s[i]<='9')
        {
            temp1=s[i]-48;
            temp=s[i-1];
            for(j=1;j<=temp1;j++)
              printf("%c",temp);
        }
    }
}
