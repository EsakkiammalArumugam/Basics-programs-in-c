// Count the special character

#include<stdio.h>
int main()
{
    char str[100];
    int count=0,i;
    scanf("%s",str);
    for(i=1;str[i]!='\0';i++)
    {
        if((str[i]>='a'&&str[i]<='z')||(str[i]>='A'&&str[i]<='Z')||(str[i]>='0'&&str[i]<='9'))
           continue;
        else
           count++;
    }
    printf("%d",count);
}
